	file = {SAGE PDF Full Text:C\:\\Users\\jonat\\Zotero\\storage\\8T7DAPIB\\Bougette et Charlier - 2018 - Antidumping and Feed-In Tariffs as Good Buddies M.pdf:application/pdf},
