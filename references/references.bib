	pages = {103--125},
	file = {Full Text PDF:C\:\\Users\\jonat\\Zotero\\storage\\ULRC73PE\\Goron - 2018 - Fighting against climate change and for fair trade finding the EU’s interest in the solar panels di.pdf:application/pdf},
}

